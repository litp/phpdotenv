{
    "name": "litp/sae-phpdotenv",
    "type": "library",
    "description": "A modified version of dotenv for SinaAppEngine",
    "keywords": ["env", "dotenv", "environment","sae"],
    "homepage": "http://github.com/litp/sae-phpdotenv",
    "license" : "BSD",
    "authors" : [
        {
            "name": "Tianpeng Li",
            "email": "litianpeng200@gmail.com",
            "homepage": "http://github.com/litp/"
        }
    ],
    "require": {
        "php": ">=5.3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "replace":{
        "vlucas/phpdotenv":"self.version"
    },
    "autoload": {
        "psr-4": {
            "Dotenv\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
